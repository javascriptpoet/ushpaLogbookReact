{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "new-cap": 0,
    "semi": [2, "never"],
    "eol-last": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "no-multiple-empty-lines": 0,

    "react/sort-comp": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 0

  }
}